{
  "name": "swoft/tcp",
  "type": "library",
  "keywords": [
    "php",
    "swoole",
    "swoft",
    "tcp",
    "protocol"
  ],
  "description": "Swoft tcp protocol component",
  "license": "Apache-2.0",
  "require": {
    "php": ">7.1",
    "swoft/stdlib": "~2.0.0"
  },
  "autoload": {
    "psr-4": {
      "Swoft\\Tcp\\": "src/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5"
  },
  "autoload-dev": {
    "psr-4": {
      "SwoftTest\\Tcp\\Unit\\": "test/unit/",
      "SwoftTest\\Tcp\\Testing\\": "test/testing/"
    }
  },
  "scripts": {
    "test": "./vendor/bin/phpunit -c phpunit.xml"
  }
}
